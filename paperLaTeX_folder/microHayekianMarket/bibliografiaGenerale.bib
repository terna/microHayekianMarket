	Eprint = {http://www.pnas.org/content/early/2010/10/04/1011942107.full.pdf+html},
	Journal = {Proceedings of the National Academy of Sciences},
	Title = {{Bankruptcy risk model and empirical tests}},
	Url = {http://www.pnas.org/content/early/2010/10/04/1011942107.abstract},
	Bdsk-File-1 = {},
