	Journal = {Proceedings of the National Academy of Sciences},
	Number = {22},
	Pages = {8830-8835},
	Title = {Coevolution of farming and private property during the early Holocene},
	Url = {http://www.pnas.org/content/110/22/8830.abstract},
	Volume = {110},
	Year = {2013},
	Bdsk-File-1 = {},
